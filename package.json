{
    "name": "natours",
    "version": "1.0.0",
    "description": "Learning node, express and mongoDB",
    "license": "ISC",
    "author": "Ali Asghar",
    "type": "commonjs",
    "main": "app.js",
    "scripts": {
        "start": "cross-env NODE_ENV=development nodemon server.js",
        "start-prod": "cross-env NODE_ENV=production nodemon server.js",
        "debug": "ndb server.js",
        "watch:js": "parcel watch ./public/js/index.js --out-dir ./public/js/distjs --out-file bundle.js"
    },
    "targets": {
        "main": false,
        "frontend": {
            "source": "./public/js/index.js",
            "distDir": "./public/js/distjs"
        }
    },
    "dependencies": {
        "axios": "^1.10.0",
        "bcryptjs": "^3.0.2",
        "cookie-parser": "^1.4.7",
        "cross-env": "^7.0.3",
        "dotenv": "^16.5.0",
        "express": "^5.1.0",
        "express-rate-limit": "^7.5.1",
        "express-xss-sanitizer": "^2.0.0",
        "helmet": "^8.1.0",
        "hpp": "^0.2.3",
        "jsonwebtoken": "^9.0.2",
        "leaflet": "^1.9.4",
        "mongo-sanitize": "^1.1.0",
        "mongoose": "^8.16.0",
        "morgan": "^1.10.0",
        "nodemailer": "^7.0.4",
        "path-to-regexp": "^8.2.0",
        "pug": "^3.0.3",
        "slugify": "^1.6.6",
        "validator": "^13.15.15"
    },
    "devDependencies": {
        "@parcel/transformer-pug": "^2.14.2",
        "parcel-bundler": "^1.12.5",
        "prettier": "^3.6.1"
    }
}
